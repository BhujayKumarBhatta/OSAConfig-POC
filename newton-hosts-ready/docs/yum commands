ceph problem 
====================

# Ceph GPG Keys

ceph_gpg_keys: 'https://download.ceph.com/keys/release.asc'



libvirt_package: libvirt-daemon-kvm

libvirt_service_name: libvirtd



python_ceph_package: python-ceph



ceph_repo_url_region: "download" # see here for other mirros http://docs.ceph.com/docs/master/install/mirrors/

ceph_yum_repo_url: "https://{{ ceph_repo_url_region }}.ceph.com/rpm-{{ ceph_stable_release }}/el7"




          "name": [
                "libvirt-daemon-kvm",
                "ceph-common",
                "python-ceph"
            ],
            "skip_broken": false,
            "state": "latest",
            "update_cache": false,
            "validate_certs": true
        }
    },
    "msg": "warning: /var/cache/yum/x86_64/7Server/epel/packages/leveldb-1.12.0-11.el7.x86_64.rpm: Header V3 RSA/SHA256 Signature, key ID 352c64e5: NOKEY\n\n\nPublic key for leveldb-1.12.0-11.el7.x86_64.rpm is not installed\n",



yum install libvirt-daemon-kvm ceph-common python-ceph 

